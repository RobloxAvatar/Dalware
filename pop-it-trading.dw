function getArea()
    for i,v in pairs(game:GetService("Workspace").Boards:GetChildren()) do
        if v.Player1.Value == game:GetService("Players").LocalPlayer or v.Player2.Value == game:GetService("Players").LocalPlayer then
            return v 
        end
    end
    return nil
end

